{
  "name": "downlodr",
  "productName": "Downlodr",
  "version": "1.3.6",
  "description": "Downlodr is a powerful, user-friendly video downloading solution that supports over 1,800 platforms, including YouTube, Vimeo, and many more. Downlodr provides a seamless experience for managing your video downloads.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "prebuild": "node scripts/fetch-yt-dlp.js",
    "package": "npm run prebuild && electron-forge package --platform=darwin",
    "make": "npm run prebuild && electron-forge make --platform=darwin",
    "make:pkg": "npm run prebuild && electron-forge make --platform=darwin --targets=@electron-forge/maker-pkg",
    "make:pkg-unsigned": "npm run package && mkdir -p out/installer && productbuild --component \"out/Downlodr-darwin-arm64/Downlodr.app\" /Applications \"out/installer/Downlodr-Installer.pkg\"",
    "make:zip": "npm run prebuild && electron-forge make --platform=darwin --targets=@electron-forge/maker-zip",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "postinstall": "npm run prebuild"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-pkg": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@felixrieseberg/electron-forge-maker-nsis": "^7.2.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "dmg-builder": "24.13.3",
    "electron": "33.3.1",
    "electron-builder-squirrel-windows": "24.13.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.5.0",
    "prettier": "3.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "Downlodr Project Team",
    "email": "downlodr@email.com"
  },
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tanstack/react-query": "^5.64.1",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "eslint-config-prettier": "^10.0.1",
    "glob": "^11.0.1",
    "lucide-react": "^0.471.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "yt-dlp-helper": "https://github.com/Talisik/yt-dlp-helper.git",
    "zustand": "^5.0.3"
  }
}
